<p id="notice"><%= notice %></p>

<h1>Visitors</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visitors.each do |visitor| %>
      <tr>
        <td><%= visitor.email %></td>
        <td><%= link_to 'Show', visitor %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

